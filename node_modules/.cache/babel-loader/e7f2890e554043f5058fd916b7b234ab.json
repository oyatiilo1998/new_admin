.categories\n    },\n    mutations: {\n        SET_CATEGORIES (state, payload) {\n            state.bonds = payload\n        }\n    },\n    actions: {\n      getCategories ({ commit }, params) {\n        return new Promise((resolve, reject) => {\n        request({\n            url: customUrl,\n            method: 'get',\n            params\n        })\n          .then(result => {\n          commit('SET_CATEGORIES', result.categories)\n          resolve(result)\n        })\n        .catch(error => {\n          reject(error)\n        })\n      })\n    },\n    getCategory ({ commit }, id) {\n      return new Promise((resolve, reject) => {\n        request({\n          url: customUrl + '/' + id,\n          method: 'get'\n        }).then(res => {\n          resolve(res)\n        }).catch(error => {\n          reject(error)\n        })\n      })\n    },\n    postCategory ({ commit }, data) {\n      return new Promise((resolve, reject) => {\n      request({\n          url: customUrl,\n          method: 'post',\n          data: data\n      })\n        .then(result => {\n        resolve(result)\n      })\n      .catch(error => {\n        reject(error)\n      })\n    })\n  },\n  updateCategory ({ commit }, data) {\n    return new Promise((resolve, reject) => {\n    request({\n        url: customUrl + '/' + data.id,\n        method: 'put',\n        data: data\n    })\n      .then(result => {\n      resolve(result)\n    })\n    .catch(error => {\n      reject(error)\n    })\n  })\n},\n  deleteCategory ({ commit }, id) {\n  return new Promise((resolve, reject) => {\n    request({\n        url: customUrl + '/' + id,\n        method: 'delete'\n    })\n      .then(result => {\n      resolve(result)\n    })\n    .catch(error => {\n      reject(error)\n    })\n  })\n}\n  }\n}\n  export default category\n"]}]}