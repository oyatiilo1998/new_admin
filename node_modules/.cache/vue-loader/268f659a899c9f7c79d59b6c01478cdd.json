ustom/Scoring/Form","sourcesContent":["<template>\n  <a-card>\n    <a-form-model :colon=\"false\" ref=\"ruleForm\" :model=\"form\" :rules=\"rules\">\n      <a-row class=\"form-row\" :gutter=\"16\">\n        <a-steps :current=\"current\">\n          <a-step v-for=\"item in steps\" :key=\"item.title\" :title=\"item.title\" />\n        </a-steps>\n        <div class=\"steps-content\">\n\n          <div class=\"step-1\" v-if=\"current === 0\">\n            <a-form-model\n              @submit=\"onSubmit\"\n              ref=\"ruleForm\"\n              :model=\"user\"\n              :rules=\"rules\"\n              :label-col=\"labelCol\"\n              :wrapper-col=\"wrapperCol\"\n            >\n              <a-row>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item ref=\"name\" :label=\"$t('firstName')\" prop=\"first_name\">\n                    <a-input\n                      v-model=\"user.first_name\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('lastName')\" prop=\"last_name\">\n                    <a-input\n                      v-model=\"user.last_name\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('middleName')\" prop=\"middle_name\">\n                    <a-input\n                      v-model=\"user.patrynomic\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('phone')\" prop=\"phone\">\n                    <a-input\n                      v-model=\"user.phone_number\" />\n                  </a-form-model-item>\