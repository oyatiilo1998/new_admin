f=\"!userId\" :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n                  <a-form-item\n                    :label=\"$t('Подтверждение пароля')\"\n                    prop=\"passwordConfirm\"\n                    has-feedback\n                    :validate-status=\"validationPasswordConfirmStatus\"\n                    :help=\"validationPasswordConfirmText\"\n                  >\n                    <a-input-password v-model=\"user.passwordConfirm\" id=\"validating\" />\n                  </a-form-item>\n                </a-col>\n              </a-row>\n            </a-form-model>\n          </div>\n          <div class=\"step-2\" v-if=\"current === 1\">\n            <a-form-model\n              @submit=\"onSubmit\"\n              ref=\"ruleFormDetails\"\n              :model=\"passportDetails\"\n              :rules=\"rulesDetails\"\n              :label-col=\"labelCol\"\n              :wrapper-col=\"wrapperCol\"\n            >\n              <a-row>\n                <a-col :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n                  <a-form-model-item\n                    :label=\"$t('inn')\"\n                    prop=\"inn\"\n                    has-feedback\n                    :validate-status=\"validationInnStatus\"\n                    :help=\"validationInnText\"\n                  >\n                    <a-input :max-length=\"9\" @keypress=\"isNum($event)\" v-model=\"passportDetails.inn\" id=\"validating\" />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item prop=\"pinfl\">\n                    <span slot=\"label\">\n                      {{ $t('director_pinfl') }}&nbsp;\n                      <a-popover placement=\"bottomRight\">\n                        <template slot=\"content\">\n                        </template>\n                        <a-icon type=\"question-circle-o\" />\n                      </a-popover>\n                    </span>\n                    <a-input\n                      v-model=\"passportDetails.pinfl\"\n                      @change=\"onPinflChange\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('passportSeries')\" prop=\"passport_serial\">\n                    <a-input\n                      @change=\"onSeriesChange\"\n                      v-model=\"passportDetails.passport_serial\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('passportNumber')\" prop=\"passport_number\">\n                    <a-input\n                      @change=\"onNumberChange\"\n                      v-model=\"passportDetails.passport_number\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item ref=\"name\" :label=\"$t('issueDate')\" prop=\"date_of_issue\">\n                    <a-date-picker\n                      value-format=\"DD-MM-YYYY\"\n                      format=\"DD-MM-YYYY\"\n                      placeholder=\"\"\n                      :disabled-date=\"disabledDateIssue\"\n                      v-model=\"passportDetails.date_of_issue\"\n                      style=\"width: 100%\"/>\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item ref=\"name\" :label=\"$t('expiryDate')\" prop=\"date_of_expiry\">\n                    <a-date-picker\n                      value-format=\"DD-MM-YYYY\"\n                      format=\"DD-MM-YYYY\"\n                      placeholder=\"\"\n                      :disabled-date=\"disabledDateExpiry\"\n                      v-model=\"passportDetails.date_of_expiry\"\n                      style=\"width: 100%\"/>\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item class=\"input-phone\" :label=\"$t('factTrue')\" prop=\"is_suitable_actual_address\">\n                    <a-switch @change=\"onSwitchChange\" default-checked v-model=\"passportDetails.is_suitable_actual_address\" />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item class=\"input-phone\" :label=\"$t('registrationAddress')\" prop=\"registration_address\">\n                    <a-input\n                      v-model=\"passportDetails.registration_address\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item class=\"input-phon\" :label=\"$t('actualAddress')\" prop=\"actual_address\">\n                    <a-input\n                      v-model=\"passportDetails.actual_address\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n                  <a-form-model-item class=\"input-phone\" :label=\"$t('country')\" prop=\"country\">\n                    <a-input\n                      :disabled=\"true\"\n                      v-model=\"country\"\n                    />\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('region')\" prop=\"region\">\n                    <a-select style=\"width:100%!important\" @change=\"getRegionCode\" v-model=\"passportDetails.region\" show-search>\n                      <a-select-option v-for=\"region in selectedRegions\" :key=\"region.code\" :value=\"region.code\">\n                        {{ region.name }}\n                      </a-select-option>\n                    </a-select>\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('district')\" prop=\"district\">\n                    <a-select style=\"width:100%!important\" v-model=\"passportDetails.district\" show-search>\n                      <a-select-option v-for=\"selectedDistrict in selectedDistricts\" :key=\"selectedDistrict.code\" :value=\"selectedDistrict.code\">\n                        {{ selectedDistrict.name }}\n                      </a-select-option>\n                    </a-select>\n                  </a-form-model-item>\n                </a-col>\n              </a-row>\n            </a-form-model>\n          </div>\n          <div class=\"step-3\" v-if=\"current === 2\">\n            <a-form-model\n              @submit=\"onSubmit\"\n              ref=\"ruleFormPhotos\"\n              :model=\"passportPhotos\"\n              :rules=\"rulesPhotos\"\n              :label-col=\"labelCol\"\n              :wrapper-col=\"wrapperCol\"\n            >\n              <a-row style=\"margin-top:20px;\">\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('passportPhoto')\" prop=\"passportPhoto\">\n                    <a-upload\n                      style=\"width:100%;\"\n                      name=\"file\"\n                      method=\"post\"\n                      list-type=\"picture-card\"\n                      class=\"avatar-uploader\"\n                      :show-upload-list=\"false\"\n                      action=\"https://api.smsuz.uz/v1/upload\"\n                      @change=\"onPassportChange\"\n                    >\n                      <img v-if=\"loadingUrlPhoto\" :src=\"loadingUrlPhoto\" alt=\"avatar\" width=\"200\" height=\"85\" />\n                      <div v-else>\n                        <a-icon :type=\"loadingPhoto ? 'loading' : 'plus'\" />\n                        <div class=\"ant-upload-text\">\n                          {{ $t('passportPhoto') }}\n                        </div>\n                      </div>\n                    </a-upload>\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('passportPhotoReg')\" prop=\"passportPhotoReg\">\n                    <a-upload\n                      name=\"file\"\n                      method=\"post\"\n                      list-type=\"picture-card\"\n                      class=\"avatar-uploader\"\n                      :show-upload-list=\"false\"\n                      action=\"https://api.smsuz.uz/v1/upload\"\n                      @change=\"onPassportRegChange\"\n                    >\n                      <img v-if=\"loadingUrlReg\" :src=\"loadingUrlReg\" alt=\"avatar\" width=\"200\" height=\"85\" />\n                      <div v-else>\n                        <a-icon :type=\"loadingReg ? 'loading' : 'plus'\" />\n                        <div class=\"ant-upload-text\">\n                          {{ $t('passportPhotoReg') }}\n                        </div>\n                      </div>\n                    </a-upload>\n                  </a-form-model-item>\n                </a-col>\n                <a-col :xs=\"24\" :md=\"12\" style=\"padding: 0 15px\">\n                  <a-form-model-item :label=\"$t('passportPhotoWith')\" prop=\"passportPhotoWith\">\n                    <a-upload\n                      name=\"file\"\n                      method=\"post\"\n                      list-type=\"picture-card\"\n                      class=\"avatar-uploader\"\n                      :show-upload-list=\"false\"\n                      action=\"https://api.smsuz.uz/v1/upload\"\n                      @change=\"onPassportWithChange\"\n                    >\n                      <img v-if=\"loadingUrlPass\" :src=\"loadingUrlPass\" alt=\"avatar\" width=\"200\" height=\"85\" />\n                      <div v-else>\n                        <a-icon :type=\"loadingPass ? 'loading' : 'plus'\" />\n                        <div class=\"ant-upload-text\">\n                          {{ $t('passportPhotoWith') }}\n                        </div>\n                      </div>\n                    </a-upload>\n                  </a-form-model-item>\n                </a-col>\n                <a-col :span=\"24\" style=\"padding: 0 15px\">\n                  <a-form-model-item class=\"input-phone\" prop=\"isChecked\">\n                    <a-switch default-checked v-model=\"passportPhotos.isChecked\" />\n                    <span>\n                      Я согласен с условиями <a href=\"https://smsuz.vercel.app/Document.pdf\"> оферты </a>на обработку персональных данных.\n                    </span>\n                  </a-form-model-item>\n                </a-col>\n              </a-row>\n            </a-form-model>\n          </div>\n        </div>\n        <div class=\"steps-action\">\n          <a-button v-if=\"current < steps.length - 1\" type=\"primary\" @click=\"next\">\n            Next\n          </a-button>\n          <a-button\n            v-if=\"current === steps.length - 1\"\n            type=\"primary\"\n            @click=\"$message.success('Processing complete!')\"\n          >\n            Done\n          </a-button>\n          <a-button v-if=\"current > 0\" style=\"margin-left: 8px\" @click=\"prev\">\n            Previous\n          </a-button>\n        </div></a-row>\n    </a-form-model>\n  </a-card>\n</template>\n\n<script>\nexport default {\n  data () {\n    return {\n      isNewForm: false,\n      form: {\n        lang: '',\n        max_investment_amount: null,\n        min_investment_amount: null,\n        title: '',\n        max_percent: null,\n        min_percent: null\n      },\n      current: 0,\n      steps: [\n        {\n          title: 'First',\n          content: 'Персональные информации'\n        },\n        {\n          title: 'Second',\n          content: 'Паспортные информации'\n        },\n        {\n          title: 'Last',\n          content: 'Подтверждение паспорта'\n        }\n      ],\n      user: {\n        first_name: '',\n        last_name: '',\n        middle_name: '',\n        phone_number: '',\n        email: '',\n        login: '',\n        password: '',\n        passwordConfirm: ''\n      },\n      passportDetails: {\n        inn: '',\n        pinfl: '',\n        passport_serial: '',\n        passport_number: '',\n        date_of_issue: '',\n        date_of_expiry: '',\n        is_suitable_actual_address: false,\n        registration_address: '',\n        actual_address: '',\n        district: '',\n        region: ''\n      },\n      passportPhotos: {\n        photo_of_passport: '',\n        photo_of_registration_in_the_passport: '',\n        photo_with_passport: '',\n        type: 'passport',\n        role_id: null,\n        isChecked: false\n      },\n      rules: {\n        'title': [{ required: true, message: 'Пожалуйста, введите', trigger: 'blur' }],\n        'max_percent': [{ required: true, message: 'Пожалуйста, введите', trigger: 'blur' }],\n        'min_percent': [{\n            required: true,\n            message: 'Пожалуйста, введите',\n            trigger: 'blur'\n          }\n          ]\n      }\n    }\n  },\n    watch: {\n    'form.min_percent' (e) {\n      if (parseInt(e) >= parseInt(this.form.max_percent)) this.form.min_percent = this.form.max_percent - 1\n    },\n     'form.max_percent' (e) {\n        if (parseInt(e) === parseInt(this.form.min_percent)) this.form.min_percent = this.form.max_percent - 1\n     },\n      'form.min_investment_amount' (e) {\n      if (parseInt(e) >= parseInt(this.form.max_investment_amount)) this.form.min_investment_amount = this.form.max_investment_amount - 1\n      console.log(e)\n    },\n     'form.max_investment_amount' (e) {\n        if (parseInt(e) === parseInt(this.form.min_investment_amount)) this.form.min_investment_amount = this.form.max_investment_amount - 1\n     }\n  },\n  methods: {\n    checkPost () {\n      let flag = null\n      this.$refs.ruleForm.validate((valid) => {\n        if (valid) {\n          flag = true\n          return true\n        } else {\n          flag = false\n          return false\n        }\n      })\n      return flag\n    },\n    next () {\n      if (this.current === 0) {\n            this.current++\n      } else if (this.current === 1) {\n            this.current++\n      }\n    },\n    prev () {\n      this.current--\n    },\n    postCategory () {\n      if (this.checkPost() === false) return -1\n      console.log(this.stringToInt())\n      this.$store.dispatch('postCategory', this.stringToInt()).then((res) => {\n        console.log(res)\n        this.$alertMessage('success', 'Tutor', 'Tutor added successfully', this)\n        this.$router.go(-1)\n      })\n    },\n    getCategory (id) {\n      this.$store.dispatch('getCategory', id).then((res) => {\n        console.log(res)\n        this.form = res.category\n      })\n    },\n    stringToInt () {\n      return { ...this.form,\n        min_investment_amount: parseInt(this.form.min_investment_amount),\n        max_investment_amount: parseInt(this.form.max_investment_amount),\n        max_percent: parseInt(this.form.max_percent),\n        min_percent: parseInt(this.form.min_percent)\n      }\n    },\n    updateCategory () {\n      if (this.checkPost() === false) return -1\n      console.log(this.stringToInt())\n      this.$store.dispatch('updateCategory', this.stringToInt()).then((res) => {\n        this.$alertMessage('success', 'Category', 'Category has updated successfully', this)\n        this.$router.go(-1)\n      })\n    },\n    cancelPost () {\n      this.$router.go(-1)\n    }\n  },\n  created () {\n    if (this.$isEmptyObj(this.$route.params)) {\n      this.isNewForm = true\n    } else this.getCategory(this.$route.params.id)\n  }\n}\n</script>\n\n<style>\n</style>\n"]}]}